{
    "name": "netflex/api",
    "description": "Netflex API library",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Alrek",
            "email": "thomas@apility.no"
        }
    ],
    "require": {
        "illuminate/support": "^8.0",
        "netflex/http": "^4.18.1",
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "illuminate/container": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Netflex\\API\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.16.x-dev"
        },
        "laravel": {
            "providers": [
                "Netflex\\API\\Providers\\APIServiceProvider"
            ],
            "aliases": {
                "API": "Netflex\\API\\Facades\\API"
            }
        }
    }
}
