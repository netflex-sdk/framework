{
    "name": "netflex/foundation",
    "description": "Netflex Foundation library",
    "type": "library",
    "license": "MIT",
    "require": {
        "illuminate/translation": "^8.0",
        "netflex/api": "^4.5",
        "netflex/support": "^4.5"
    },
    "autoload": {
        "psr-4": {
            "Netflex\\Foundation\\": ""
        },
        "files": [
            "helpers.php"
        ]
    },
    "require-dev": {
        "illuminate/cache": "^8.0",
        "illuminate/container": "^8.0",
        "illuminate/filesystem": "^8.0",
        "larapack/dd": "^1.1",
        "vlucas/phpdotenv": "^5.2",
        "php": "^7.4|^8.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.5.x-dev"
        },
        "laravel": {
            "providers": [
                "Netflex\\Foundation\\Providers\\FoundationServiceProvider",
                "Netflex\\Foundation\\Providers\\TranslationServiceProvider"
            ]
        }
    }
}
